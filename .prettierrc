{
  "quoteProps": "consistent",
  "trailingComma": "all",
  "tabWidth": 2,
  "singleAttributePerLine": true,
  "printWidth": 80,
  "jsxSingleQuote": true,
  "endOfLine": "lf",
  "arrowParens": "always",
  "singleQuote": true,
  "semi": true,
  "bracketSameLine": false,
  "bracketSpacing": true,
  "cursorOffset": -1,
  "editorconfig": true,
  "embeddedLanguageFormatting": "auto",
  "htmlWhitespaceSensitivity": "css",
  "insertPragma": false,
  "plugins": ["prettier-plugin-tailwindcss"],
  "proseWrap": "preserve",
  "requirePragma": false,
  "useTabs": false
}